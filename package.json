	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"printWidth": 100,
		"semi": true,
		"singleQuote": true,
		"tabWidth": 4,
		"trailingComma": "all",
		"useTabs": true
	},
	"scripts": {
		"dev": "yarn dev:local",
		"dev:local": "cross-env DEBUG=vite-ssg:* vite",
		"dev:vercel": "vite build && vercel dev",
		"build": "vue-tsc --noEmit && cross-env DEBUG=vite-ssg:* vite-ssg build",
		"preview": "vite preview",
		"prettier": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx,vue}\""
	},
